{
  "name": "ol3-custom-build",
  "description": "tool for build custom openlayers library.",
  "version": "0.1.0",
  "author": "Victor Rubinat<vrubinat@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vrubinat/ol3-build-custom.git"
  },
  "goog": {
    "url": "https://github.com/google/closure-library.git"
  },
  "ol3": {
    "url": "https://github.com/openlayers/ol3.git",
    "branch": "master"
  },
  "build": {
    "deps": false,
    "update": true,
    "gzip": true,
    "compile": true,
    "sourceMap": false,
    "libs": false,
    "html": true,
    "onlyOl3":true,
    "externs": [
      "lib/ol3/externs/jquery-1.9.js"
    ]
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "async": "^0.9.0",
    "fs-extra": "^0.13.0",
    "grunt": "^0.4.5",
    "grunt-closure-tools": "^0.9.7",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-compress": "^0.13.0",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-cssmin": "^0.11.0",
    "grunt-downloadfile": "^0.3.2",
    "grunt-file-append": "0.0.5",
    "grunt-git": "^0.3.2",
    "grunt-web-server": "^1.2.3",
    "grunt-zip": "^0.16.2",
    "jsdoc": "^3.3.0-alpha13",
    "nomnom": "^1.8.1",
    "rbush": "^1.3.4",
    "walk": "^2.3.4"
  }
}
